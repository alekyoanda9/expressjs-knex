{
  "name": "expressjs-knex",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate:make": "knex --knexfile ./knex/knexfile.ts migrate:make",
    "migrate:mysql": "knex --knexfile ./knex/knexfile.ts --env main migrate:latest",
    "migrate:pg": "knex --knexfile ./knex/knexfile.ts --env secondary migrate:latest",
    "migrate:rollback:mysql": "knex --knexfile ./knex/knexfile.ts --env main migrate:rollback",
    "migrate:rollback:pg": "knex --knexfile ./knex/knexfile.ts --env secondary migrate:rollback"
  },
  "author": "Alek Yoanda",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mysql2": "^3.15.2",
    "pg": "^8.16.3",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/knex": "^0.15.2",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
